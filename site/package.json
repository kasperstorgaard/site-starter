{
  "name": "@site-starter/site",
  "version": "0.1.0",
  "type": "module",
  "description": "the main astro site",
  "main": "index.js",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasperstorgaard/site-starter.git"
  },
  "keywords": [
    "astro"
  ],
  "author": "kasperstorgaard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kasperstorgaard/site-starter/issues"
  },
  "homepage": "https://github.com/kasperstorgaard/site-starter#readme",
  "dependencies": {
    "astro": "^0.22.20",
    "astro-icon": "^0.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "babel-loader": "^8.2.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^4.6.2",
    "vite": "^2.7.13"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": ["prettier", "import", "ava"],
    "rules": {
      "semi": ["error", "always"],
      "object-curly-spacing": ["warn", "always"],
      "comma-dangle": ["warn", "always-multiline"],
      "quotes": ["warn", "single"]
    }
  }
}
