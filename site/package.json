{
  "name": "site",
  "version": "0.1.0",
  "description": "the main site",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasperstorgaard/site-starter.git"
  },
  "keywords": [
    "astro",
    "JAMStack",
    "vite"
  ],
  "author": "kasperstorgaard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kasperstorgaard/site-starter/issues"
  },
  "homepage": "https://github.com/kasperstorgaard/site-starter#readme",
  "scripts": {
    "build": "npx astro build",
    "dev": "astro dev",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "astro": "^0.25.4",
    "astro-icon": "^0.6.1",
    "lit": "^2.2.2",
    "netlify-cli": "^10.0.0",
    "picocolors": "^1.0.0",
    "source-map": "^0.6.1",
    "vite": "^2.9.0"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "import"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "quotes": [
        "warn",
        "single"
      ]
    }
  }
}
